{
  "name": "client",
  "version": "0.0.1",
  "description": "Storm the front project client side",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack/client/dev.js --client-only",
    "build:client:prod": "cross-env NODE_ENV=production OMIT_SOURCEMAP=true webpack --config webpack/client/prod.js --client-only",
    "build:client:dev": "cross-env NODE_ENV=development webpack --config webpack/client/dev.js --client-only",
    "build:server:prod": "cross-env NODE_ENV=production OMIT_SOURCEMAP=true webpack --config webpack/server/prod.js",
    "build:server:dev": "cross-env NODE_ENV=development webpack --config webpack/server/dev.js",
    "eslint": "eslint ./src --ext .js,.jsx --max-warnings 5",
    "test": "jest",
    "lint-staged": "lint-staged"
  },
  "husky": {	
    "hooks": {	
      "pre-push": "npm run eslint && jest",	
      "pre-commit": "npm run lint-staged"	
    }	
  },
  "keywords": [
    "react",
    "MCC",
    "boilerplate",
    "webpack",
    "babel"
  ],
  "author": "MCC React",
  "license": "none",
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.6.2",
    "@babel/preset-react": "7.0.0",
    "autoprefixer": "9.6.1",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "3.2.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint": "6.5.1",
    "eslint-config-airbnb": "18.0.1",
    "eslint-import-resolver-webpack": "0.11.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.15.0",
    "file-loader": "4.2.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.0.7",
    "jest": "24.9.0",
    "mini-css-extract-plugin": "0.8.0",
    "node-sass": "4.12.0",
    "postcss-loader": "3.0.0",
    "react-hot-loader": "4.12.14",
    "sass-loader": "8.0.0",
    "style-loader": "1.0.0",
    "webpack": "4.41.0",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "3.8.1"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "copy-webpack-plugin": "^5.0.4",
    "cors": "^2.8.5",
    "cross-env": "6.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "express": "^4.17.1",
    "isomorphic-fetch": "2.2.1",
    "lint-staged": "^9.4.1",
    "prettier": "^1.18.2",
    "prop-types": "15.7.2",
    "react": "16.10.1",
    "react-dom": "16.10.1",
    "react-helmet": "^5.2.1",
    "react-helmet-async": "^1.0.3",
    "react-loadable-ssr-addon": "^0.1.9",
    "react-redux": "7.1.1",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "redux": "4.0.4",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "setupFiles": [
      "./setupJest.js"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ]
  }
}
